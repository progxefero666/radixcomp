generator client {
    provider = "prisma-client-js"
    output   = "../generated/prisma"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

// ----------------- MODELS -----------------

model Proglanguage {
    id          Int     @db.VarChar(16)
    name        String  @unique @db.VarChar(16)
    @@map("proglanguage")
}
